{
    "name": "tms320c54x-support",
    "displayName": "TMS320C54x DSP Assembly",
    "description": "Provides syntax highlighting and snippets for TMS320C54x DSP assembly language",
    "version": "0.0.1",
    "publisher": "Xiexiaocheng",
    "homepage": "https://github.com/5966466/TMS320C54x-support",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/5966466/TMS320C54x-support"
    },
    "bugs": {
        "url": "https://github.com/5966466/TMS320C54x-support/issues",
        "email": "w5966466@gmail.com"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tms320c54x",
                "aliases": [
                    "TMS320C54x DSP Assembly",
                    "tms320c54x"
                ],
                "extensions": [
                    ".asm",
                    ".cmd"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tms320c54x",
                "scopeName": "source.tms320c54x",
                "path": "./syntaxes/tms320c54x.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "tms320c54x",
                "path": "./snippets/snippets.json"
            }
        ],
        "themes": [
            {
                "label": "TMS320C54x Color Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/TMS320C54x-color-theme.json"
            }
        ]
    }
}
